<section class="sponsor-section">
  {% for sponsor in section.blocks %}
    {% include 'sponsor' %}
  {% endfor %}
</section>

{% schema %}
  {
    "name": "Sponsor",
    "tag": "section",
    "blocks": [
      {
        "type": "content",
        "name": "Brand Logos",
        "limit": 8,
        "settings": [
          {
            "type": "image_picker",
            "label": "Upload Brand Logo",
            "id": "sponsor_brand_logo"
          }
        ]
      }
    ]
  }
{% endschema %}